    "redux-persist-indexeddb-storage": "^1.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Momomash/FrontierTime.git"
  },
  "author": "Maria Kondaurova",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Momomash/FrontierTime/issues"
  },
  "homepage": "https://github.com/Momomash/FrontierTime#readme",
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.app",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.app",
        "preset": "iPhone 7"
      }
    }
  }
