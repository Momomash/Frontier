{
  "name": "frontier_time",
  "version": "1.0.0",
  "description": "Anti-cafe visitor time control application",
  "main": "./src/index.ts",
  "scripts": {
    "test": "jest",
    "build": "npx webpack --mode production",
    "prepublishOnly": "npm run build",
    "calc": "npx ts-node src/lesson2",
    "start": "npx webpack-dev-server --mode development --open --hot",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "loki:update": "npm run build-storybook && npx loki update --requireReference --reactUri file:./storybook-static",
    "loki:approve": "npx loki approve",
    "test:loki": "npm run build-storybook && npx loki --requireReference --reactUri file:./storybook-static",
    "check": "npm run lint && npm test && npm run test:loki"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint"
    }
  },
  "devDependencies": {
    "@babel/cli": "7.10.5",
    "@babel/core": "7.11.0",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/polyfill": "7.10.4",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@storybook/addon-actions": "5.3.19",
    "@storybook/addon-docs": "5.3.19",
    "@storybook/addon-knobs": "5.3.19",
    "@storybook/addon-links": "5.3.19",
    "@storybook/addons": "5.3.19",
    "@storybook/react": "5.3.19",
    "@storybook/source-loader": "5.3.19",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/jest": "26.0.8",
    "@types/react": "16.9.44",
    "@types/react-dom": "16.9.8",
    "@types/react-redux": "7.1.9",
    "@types/react-router-dom": "5.1.5",
    "@types/react-test-renderer": "16.9.2",
    "@typescript-eslint/eslint-plugin": "3.7.1",
    "@typescript-eslint/parser": "3.7.1",
    "@webpack-cli/init": "0.3.0",
    "babel-cli": "6.26.0",
    "babel-jest": "26.2.2",
    "babel-loader": "8.1.0",
    "cross-env": "7.0.2",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "7.6.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.20.5",
    "eslint-plugin-standard": "4.0.1",
    "html-webpack-plugin": "4.3.0",
    "install": "0.13.0",
    "jest": "26.2.2",
    "jest-emotion": "10.0.32",
    "prettier": "2.0.5",
    "ts-node": "8.10.2",
    "typescript": "3.9.7",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12"
  },
  "dependencies": {
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "@reduxjs/toolkit": "1.4.0",
    "@types/redux-persist": "^4.3.1",
    "core-js": "3.6.5",
    "emotion": "10.0.27",
    "lodash": "4.17.19",
